<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="130.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.rshu.lab.controller.RecordAddEditController">
    <children>
        <Label layoutX="64.0" layoutY="42.0" text="Pick new return date" AnchorPane.bottomAnchor="90.0"
               AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <DatePicker fx:id="returnDatePicker" AnchorPane.bottomAnchor="65.0" AnchorPane.leftAnchor="20.0"
                    AnchorPane.rightAnchor="20.0"/>
        <Button fx:id="saveEdit" onAction="#handleSaveEdit" mnemonicParsing="false" prefWidth="100.0" text="Save"
                AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0"/>
        <Button fx:id="cancel" onAction="#handleCancel" layoutX="10.0" layoutY="10.0" mnemonicParsing="false"
                prefWidth="100.0" text="Cancel" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="20.0"/>
    </children>
</AnchorPane>
